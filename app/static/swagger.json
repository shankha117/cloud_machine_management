  "servers": [
    {
      "url": "http://localhost:8081/cmm/api/v1",
      "description": "Local"
    }
  ],
  "components": {
    "parameters": {
      "offsetParam": {
        "in": "query",
        "name": "offset",
        "required": false,
        "description": "The number of items to skip before starting to collect the result set.",
        "schema": {
          "type": "integer",
          "minimum": 0,
          "default": 0
        }
      },
      "limitParam": {
        "in": "query",
        "name": "limit",
        "required": false,
        "description": "The numbers of items to return.",
        "schema": {
          "type": "integer",
          "minimum": 1,
          "maximum": 50,
          "default": 10
        }
      },
      "sort_order": {
        "in": "query",
        "name": "sort_order",
        "description": "Optional filter based on status",
        "required": false,
        "schema": {
          "type": "string",
          "enum": [
            "asc",
            "desc"
          ]
        }
      },
      "include_aggregate": {
        "in": "query",
        "name": "include_aggregate",
        "description": "Optional filter whether to include_aggregate files",
        "required": false,
        "schema": {
          "type": "boolean",
          "default": false
        }
      },
      "parent_doc_id": {
        "in": "query",
        "name": "parent_doc_id",
        "description": "Optional filter on parent_doc_id of files",
        "required": false,
        "schema": {
          "type": "string"
        }
      },
      "tag_name": {
        "in": "query",
        "name": "tag_name",
        "description": "Optional filter based on tag_name",
        "required": false,
        "schema": {
          "type": "string",
          "_comment": "multiple values comma seperated",
          "example": "Batch1"
        }
      },
      "config_type_a": {
        "in": "query",
        "name": "locale",
        "description": "Optional filter based on locale type",
        "required": false,
        "schema": {
          "type": "string"
        }
      },
      "config_type_b": {
        "in": "query",
        "name": "domain",
        "description": "Optional filter based on domain type",
        "required": false,
        "schema": {
          "type": "string"
        }
      },
      "config_type_c": {
        "in": "query",
        "name": "document_type",
        "description": "Optional filter based on document type",
        "required": false,
        "schema": {
          "type": "string"
        }
      },
      "AuthToken": {
        "in": "header",
        "name": "Authorization",
        "description": "Token from keycloak, format - Bearer",
        "required": false,
        "schema": {
          "type": "string"
        }
      },
      "user_id": {
        "in": "header",
        "name": "user_id",
        "description": "User id if security is disabled",
        "required": false,
        "schema": {
          "type": "string"
        }
      },
      "status_filter": {
        "in": "query",
        "name": "status",
        "description": "Optional filter based on status",
        "required": false,
        "schema": {
          "type": "string",
          "enum": [
            "NEW",
            "WAITING",
            "SCHEDULED",
            "RUNNING",
            "FAILED",
            "SUCCESS"
          ]
        }
      },
      "sort_by": {
        "in": "query",
        "name": "sort_by",
        "description": "Sort based on date",
        "required": false,
        "schema": {
          "type": "string",
          "enum": [
            "created_date",
            "modified_date"
          ]
        }
      },
      "highlighted": {
        "in": "query",
        "name": "highlighted",
        "description": "Get the hightlighted PDF",
        "required": false,
        "schema": {
          "type": "boolean",
          "default": false
        }
      }
    },
    "securitySchemes": {
      "Authorization_token": {
        "type": "apiKey",
        "name": "Authorization",
        "in": "header",
        "description": "Provide the Auth 2.0 token from keycloak, format - Bearer"
      }
    },
    "schemas": {
      "Projects.create.input": {
        "type": "object",
        "required": [
          "name",
          "desc"
        ],
        "properties": {
          "name": {
            "type": "string",
            "example": "sample project"
          },
          "desc": {
            "type": "string",
            "example": "sample project for legal"
          },
          "locale": {
            "type": "string",
            "example": "USA"
          },
          "domain": {
            "type": "string",
            "example": "Legal"
          },
          "type": {
            "type": "string",
            "example": "MSA"
          }
        },
        "xml": {
          "name": "Projects"
        }
      },
      "DocJob": {
        "type": "object",
        "required": [
          "document_name",
          "project_id"
        ],
        "properties": {
          "projects": {
            "type": "object",
            "properties": {
              "hits": {
                "type": "object",
                "properties": {
                  "total": {
                    "type": "object",
                    "properties": {
                      "value": {
                        "type": "integer",
                        "example": "3"
                      }
                    }
                  },
                  "hits": {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "properties": {
                        "_id": {
                          "type": "string",
                          "example": "eJay824BdehpG9CvrCoH"
                        },
                        "_source": {
                          "type": "object",
                          "properties": {
                            "document_id": {
                              "type": "string",
                              "example": "pMnGf28B1CuG2fQxfi__"
                            },
                            "project_id": {
                              "type": "string",
                              "example": "dsm8f28B1CuG2fQxfCc5"
                            },
                            "status": {
                              "type": "string",
                              "example": "SUCCESS"
                            },
                            "created_by": {
                              "type": "integer",
                              "example": "1"
                            },
                            "start_time": {
                              "type": "string",
                              "example": ""
                            },
                            "created_date": {
                              "type": "string",
                              "example": "2019-12-11T06:43:23.246Z"
                            },
                            "modified_date": {
                              "type": "string",
                              "example": "2019-12-11T06:43:23.246Z"
                            },
                            "end_time": {
                              "type": "string",
                              "example": "2019-12-11T06:43:23.246Z"
                            },
                            "scheduled_time": {
                              "type": "string",
                              "example": "2019-12-11T06:43:23.246Z"
                            },
                            "celery_job_info": {
                              "type": "string",
                              "example": "abcd-efg-xyz"
                            }
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        },
        "xml": {
          "name": "DocJob"
        }
      },
      "ProjectResultResponse": {
        "type": "object",
        "properties": {
          "result": {
            "type": "string",
            "example": "<base64>"
          }
        },
        "xml": {
          "name": "ProjectResultResponse"
        }
      },
      "TagResultResponse": {
        "type": "object",
        "properties": {
          "_id": {
            "type": "string",
            "example": "fJay824BdehpG9CvrCoH"
          },
          "_source": {
            "type": "object",
            "properties": {
              "project_id": {
                "type": "string",
                "example": "fJay824BdehpG9CvrCoH"
              },
              "tag_name": {
                "type": "string",
                "example": "batch1234"
              },
              "status": {
                "type": "string",
                "example": "RUNNING"
              },
              "created_time": {
                "type": "string",
                "example": "2019-12-11T06:43:23.246Z"
              },
              "updated_time": {
                "type": "string",
                "example": "2019-12-11T06:43:23.246Z"
              },
              "statistics": {
                "type": "object",
                "properties": {
                  "total": {
                    "type": "integer",
                    "example": 20
                  },
                  "success": {
                    "type": "integer",
                    "example": 10
                  },
                  "failure": {
                    "type": "integer",
                    "example": 5
                  },
                  "running": {
                    "type": "integer",
                    "example": 5
                  }
                }
              }
            }
          }
        },
        "xml": {
          "name": "ProjectResponse"
        }
      },
      "ProjectResponse": {
        "type": "object",
        "properties": {
          "project_details": {
            "type": "object",
            "properties": {
              "_id": {
                "type": "string",
                "example": "eJay824BdehpG9CvrCoH"
              },
              "_source": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string",
                    "example": "Sample project"
                  },
                  "description": {
                    "type": "string",
                    "example": "Example project for legal docs"
                  },
                  "type": {
                    "type": "object",
                    "properties": {
                      "domain": {
                        "type": "string",
                        "example": "Finance"
                      },
                      "location": {
                        "type": "string",
                        "example": "USA"
                      },
                      "doc_type": {
                        "type": "string",
                        "example": "Legal"
                      }
                    }
                  },
                  "status": {
                    "type": "string",
                    "example": "ACTIVE"
                  },
                  "created_by": {
                    "type": "integer",
                    "example": "1"
                  },
                  "output": {
                    "type": "string",
                    "example": "null"
                  },
                  "created_date": {
                    "type": "string",
                    "example": "2019-12-11T06:43:23.246Z"
                  },
                  "modified_date": {
                    "type": "string",
                    "example": "2019-12-11T06:43:23.246Z"
                  }
                }
              }
            }
          }
        },
        "xml": {
          "name": "ProjectResponse"
        }
      },
      "ProjectUpdateResponse": {
        "type": "object",
        "properties": {
          "project_details": {
            "type": "object",
            "properties": {
              "_id": {
                "type": "string"
              },
              "_source": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string",
                    "example": "project new"
                  },
                  "description": {
                    "type": "string",
                    "example": "My new project"
                  },
                  "type": {
                    "type": "object",
                    "properties": {
                      "domain": {
                        "type": "string",
                        "example": "Finanace"
                      },
                      "location": {
                        "type": "string",
                        "example": "USA"
                      },
                      "doc_type": {
                        "type": "string",
                        "example": "Legal"
                      }
                    }
                  },
                  "status": {
                    "type": "string",
                    "example": "Legal"
                  },
                  "created_by": {
                    "type": "integer",
                    "example": "1"
                  },
                  "output": {
                    "type": "string",
                    "example": ""
                  },
                  "created_date": {
                    "type": "string",
                    "example": "2019-12-11T06:43:23.246Z"
                  },
                  "modified_date": {
                    "type": "string",
                    "example": "2019-12-11T06:43:23.246Z"
                  }
                }
              }
            }
          }
        },
        "xml": {
          "name": "ProjectResponse"
        }
      },
      "ProjectDeleteResponse": {
        "type": "object",
        "properties": {
          "message": {
            "type": "string",
            "example": "Deleted the project and 2 documents"
          }
        },
        "xml": {
          "name": "ProjectResponse"
        }
      },
      "ProjectInvalid": {
